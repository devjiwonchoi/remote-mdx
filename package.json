{
  "name": "remote-mdx",
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "build": "bunchee",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "lint-fix": "prettier --write !pnpm-lock.yaml .",
    "prepublishOnly": "pnpm build -m"
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "vfile": "^5.3.7",
    "vfile-matter": "^3.0.1"
  },
  "devDependencies": {
    "@swc/core": "^1.4.8",
    "@swc/jest": "^0.2.36",
    "@types/babel__code-frame": "^7.0.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^16.18.90",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "bunchee": "^4.4.8",
    "jest": "^29.7.0",
    "next": "^14.1.3",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.2",
    "unist-util-remove": "^3.1.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": [
        "@swc/jest",
        {
          "jsc": {
            "transform": {
              "react": {
                "runtime": "automatic"
              }
            }
          }
        }
      ]
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "verbose": true
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "es5"
  },
  "files": [
    "dist",
    "rsc",
    "serialize"
  ],
  "license": "MIT",
  "author": "Jiwon Choi",
  "repository": "https://github.com/devjiwonchoi/remote-mdx.git",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./rsc": {
      "import": "./rsc/index.js",
      "types": "./rsc/index.d.ts"
    },
    "./serialize": {
      "import": "./serialize/index.js",
      "types": "./serialize/index.d.ts"
    }
  }
}